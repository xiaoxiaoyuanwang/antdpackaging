{
  "name": "antdpackaging",
  "version": "0.2.0",
  "description": "antd components library",
  "author": "xiaoxiaoyuanwang",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "React",
    "Form",
    "antd"
  ],
  "homepage": "https://github.com/xiaoxiaoyuanwang/antdpackaging.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoxiaoyuanwang/antdpackaging.git"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "npm run clean && npm run build-ts && npm run build-css",
    "build-link": "npm run build-ts && npm run build-css",
    "build-ts": "tsc -p tsconfig.build.json",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.12.3",
    "classnames": "^2.2.6",
    "moment": "^2.29.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
